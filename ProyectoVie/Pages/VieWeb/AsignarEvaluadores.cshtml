<section class="proposal-form">
  <header class="header">
    <img src="https://cdn.builder.io/api/v1/image/assets/TEMP/44bdabc5dc04e088cab089076d6baf55d3dead093b10fa42befb76e64c4e363a?placeholderIfAbsent=true&apiKey=9365bb61cb704b4087925543482a97b7" alt="Company Logo" class="logo" />
    <div class="search-bar" role="search">
      <label for="search-input" class="visually-hidden">Search</label>
      <input type="search" id="search-input" placeholder="Search..." aria-label="Search input" />
    </div>
    <img src="https://cdn.builder.io/api/v1/image/assets/TEMP/a321d79a8a4f596dffd7adaf41c30f935804fb31ba00f6388074cf8cf29647d8?placeholderIfAbsent=true&apiKey=9365bb61cb704b4087925543482a97b7" alt="Notification" class="icon" />
    <img src="https://cdn.builder.io/api/v1/image/assets/TEMP/cdce405d82c875d3eb6bad8a7b3e9fc34012a91f11b577fd60885827935cc3d3?placeholderIfAbsent=true&apiKey=9365bb61cb704b4087925543482a97b7" alt="Messages" class="icon" />
    <img src="https://cdn.builder.io/api/v1/image/assets/TEMP/9f4d802b5cbbc4a1eb1afe3010114a3b91ade4b91bbf9d6cdfc7a6be30558777?placeholderIfAbsent=true&apiKey=9365bb61cb704b4087925543482a97b7" alt="User Avatar" class="user-avatar" />
  </header>
  
  <div class="main-content">
    <nav class="sidebar">
      <img src="https://cdn.builder.io/api/v1/image/assets/TEMP/6212a3613f0516fcaa8c93f867af1d4f495414a03d50884bcf74585b7e977da2?placeholderIfAbsent=true&apiKey=9365bb61cb704b4087925543482a97b7" alt="My Proposals" class="sidebar-icon" />
      <div class="sidebar-text">My Proposals</div>
    </nav>
    
    <main class="form-container">
      <div class="form-header">
        <div class="form-title-wrapper">
          <div class="form-title-column">
            <div class="form-title-content">
              <h1 class="form-title">Proposal Registration Form</h1>
              <h2 class="form-subtitle">I. Academic Instance Data:</h2>
            </div>
          </div>
          <div class="logo-column">
            <div class="logo-wrapper">
              <img src="https://cdn.builder.io/api/v1/image/assets/TEMP/d96e170b2f1f5c5e8e4e7eb5a97a77f75015207f13b9ae84b8ff7de14645a58e?placeholderIfAbsent=true&apiKey=9365bb61cb704b4087925543482a97b7" alt="Small Logo" class="logo-small" />
              <img src="https://cdn.builder.io/api/v1/image/assets/TEMP/2b928fd1f2b0dc3534e7f0c9c20497d639823d3884a3ab0c911a5d4b9355d917?placeholderIfAbsent=true&apiKey=9365bb61cb704b4087925543482a97b7" alt="Large Logo" class="logo-large" />
            </div>
          </div>
        </div>
      </div>
      
      <form>
        <div class="form-field">
          <label for="agreement-number">a) School agreement number:</label>
          <input type="text" id="agreement-number" value="12-2017" readonly />
        </div>
        
        <div class="form-field-date">
          <label for="submission-date">b) Submission date:</label>
          <input type="date" id="submission-date" value="2024-09-10" readonly />
        </div>
        
        <h2 class="form-section-title">II. Project Data:</h2>
        
        <div class="form-row">
          <label for="project-name" class="form-label">a) Project name:</label>
          <input type="text" id="project-name" class="form-input" required />
        </div>
        
        <div class="form-row-select">
          <label for="extension-type" class="form-label-select">b) Extension type:</label>
          <div class="form-select">
            <select id="extension-type" required>
              <option value="">Choose an option</option>
              <option value="other">Other (specify)</option>
            </select>
            <img src="https://cdn.builder.io/api/v1/image/assets/TEMP/28b5f665aa2d8aa74c6317c41ae5d44c7b94ab8cb1387b86e29e50cc1eb54cf4?placeholderIfAbsent=true&apiKey=9365bb61cb704b4087925543482a97b7" alt="Dropdown" class="form-select-icon" />
          </div>
        </div>
        
        <div class="form-field-ods">
          <label for="sustainable-goals">c) Sustainable Development Goal:</label>
        </div>
        
        <textarea id="sustainable-goals" class="form-textarea" rows="4" placeholder="Specify the goals..."></textarea>

        <div class="form-actions">
          <button type="button" class="btn-cancel">Cancel</button>
          <button type="submit" class="btn-submit">Submit</button>
        </div>
      </form>
    </main>
  </div>
</section>

